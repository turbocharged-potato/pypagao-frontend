{
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-one-expression-per-line": "off",
      "no-param-reassign": ["error", { "props": false }],
      "no-console": "off",
      "react-app/jsx-a11y/href-no-hash": "off"
    }
}
